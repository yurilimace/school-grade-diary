// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model StudendClass {
  id String  @unique @default(uuid())
  name String @default("")
  students Student[]
}

model Student {
  id String @unique @default(uuid())
  name String
  age Int
  StudentClass StudendClass @relation(fields: [studentClassId],references: [id])
  studentClassId String
}
